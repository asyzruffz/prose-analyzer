{
  "name": "prose",
  "displayName": "Prose",
  "description": "Syntax highlighting for prose lang",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "prose",
      "aliases": ["Prose", "prose"],
      "extensions": [".prs"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "prose",
      "scopeName": "source.prose",
      "path": "./syntaxes/prose.tmLanguage.json"
    }]
  }
}
